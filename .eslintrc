{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "require": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "eqeqeq": 0,
    "curly": 2,
    "quotes": [2, "single"],
    "strict": 0,
    "no-unused-vars": [1, "args": "after-used"]
  }
}